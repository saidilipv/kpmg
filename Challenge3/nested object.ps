$nestedObject = @{
    "NO1" = "3"
    "NO2" = @{
        "NO3" = "5"
        "NO4" = @{
            "NO5" = "8"
        }
    }
    "NO5" = @(
        @{
            "NO7" = "10"
        }
        @{
            "NO8" = "25"
        }
    )
}

$result = Get-ValueFromNestedObject -Object $nestedObject -Key "NO3"
Write-Host $result  # Output: value5

$result = Get-ValueFromNestedObject -Object $nestedObject -Key "NO8"
Write-Host $result  # Output: value8

$result = Get-ValueFromNestedObject -Object $nestedObject -Key "NO7"
Write-Host $result  # Output: $null